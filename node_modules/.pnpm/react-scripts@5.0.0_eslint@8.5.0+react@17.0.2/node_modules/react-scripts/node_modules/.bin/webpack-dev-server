#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-dev-server@4.8.1_webpack@5.72.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-dev-server@4.8.1_webpack@5.72.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
